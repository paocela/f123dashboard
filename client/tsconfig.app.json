{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "@angular/localize"
    ],
    "paths": {
      "@docs-components/*": [
        "./src/components/*"
      ],
      "@f123dashboard/shared": [
        "../shared/src/index.ts"
      ],
      "jszip": [
        "./node_modules/jszip/dist/jszip.min.js"
      ],
      "node_modules/chart.js/dist/types/basic": [
        "../node_modules/chart.js/dist/types/basic.d.ts"
      ],
      "node_modules/chart.js/dist/types/utils": [
        "../node_modules/chart.js/dist/types/utils.d.ts"
      ]
    }
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
